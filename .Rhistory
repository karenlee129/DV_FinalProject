require(DT)
shinyServer(function(input, output) {
# Generate the data frame from the server
df1 <- eventReactive(input$clicks1, {df <- data.frame(fromJSON(getURL(URLencode('skipper.cs.utexas.edu:5001/rest/native/?query="
select COUNTRY, TTL_G_COUNTRY_OVERALL/TTL_MEDALS_COUNTRY_OVERALL AS GP, TTL_S_COUNTRY_OVERALL/TTL_MEDALS_COUNTRY_OVERALL AS SP, TTL_B_COUNTRY_OVERALL/TTL_MEDALS_COUNTRY_OVERALL AS BP
from olympics
group by country "'), httpheader=c(DB='jdbc:oracle:thin:@sayonara.microlab.cs.utexas.edu:1521:orcl', USER='C##cs329e_kwl377', PASS='orcl_kwl377', MODE='native_mode', MODEL='model', returnDimensions = 'False', returnFor = 'JSON'), verbose = TRUE)))
})
# Code for input the year
#YEAR <- reactive({input$yr})
# Code for the check box
checkBoxes <- eventReactive({input$country}, {
if (input$country == "All" || is.null(input$country))
{
checkBoxes = c("France","Germany", "Italy", "Japan", "United Kingdom", "United States", "Australia", "Hungary", "Sweden")
}
else
{
checkBoxes = input$country
}
}, ignoreNULL = FALSE)
# First ggplot------------------------------------------------------
#sum([Ttl G Country Overall])/sum([Ttl Medals Country Overall])
output$distPlot1 <- renderPlot({
# Filter the year and the brand of the car based on the check box
#scatterplot <- df1() %>% select(COUNTRY, TTL_G_COUNTRY_OVERALL/TTL_MEDALS_COUNTRY_OVERALL AS GP, TTL_S_COUNTRY_OVERALL/TTL_MEDALS_COUNTRY_OVERALL AS SP, TTL_B_COUNTRY_OVERALL/TTL_MEDALS_COUNTRY_OVERALL AS BP ) %>% group_by(COUNTRY) %>% subset(YEAR %in% YEAR()) %>% subset(MANUFACTURER %in% checkBoxes())
plot1 <- ggplot() +
coord_cartesian() +
scale_x_discrete() +
scale_y_continuous() +
labs(title='Medal Percentage for Specific Countries') +
labs(x="Country", y=paste("GP")) +
layer(data=scatterplot,
mapping=aes(x=country, y=as.numeric(GP)),
stat="identity",
stat_params=list(),
geom="point",
#geom_params=list(),
position=position_identity()
)
plot1
})
# End First ggplot---------------------------------------------------
#***********************************************************************
})
